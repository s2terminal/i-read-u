{
  "name": "i-read-u",
  "author": "s2terminal",
  "description": "Extracting commands from README markdown file.",
  "version": "1.0.2",
  "bin": {
    "i-read-u": "./bin/ireadu.js",
    "ireadu": "./bin/ireadu.js"
  },
  "main": "./bin/ireadu.js",
  "scripts": {
    "test": "jest --coverage",
    "prepare": "webpack --config webpack.common.js",
    "webpack-watch": "webpack --config webpack.dev.js --watch",
    "webpack-size": "webpack --config webpack.dev.js -p",
    "lint": "tslint --config tslint.json src/index.ts",
    "lint:fix": "tslint --config tslint.json --fix src/index.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/inquirer": "0.0.42",
    "@types/jest": "^23.3.14",
    "@types/marked": "^0.4.2",
    "@types/node": "^10.14.8",
    "jest": "^23.6.0",
    "prettier": "^1.18.2",
    "size-plugin": "^1.2.0",
    "terser": "^3.17.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.5.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "commander": "^2.20.0",
    "inquirer": "^6.3.1",
    "marked": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s2terminal/i-read-u.git"
  },
  "bugs": {
    "url": "https://github.com/s2terminal/i-read-u/issues"
  },
  "homepage": "https://s2terminal.github.io/i-read-u/",
  "keywords": [
    "README",
    "markdown",
    "command",
    "parser"
  ],
  "license": "MIT"
}
