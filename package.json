{
  "name": "i-read-u",
  "author": "s2terminal",
  "description": "Read README markdown file, and execute written commands.",
  "version": "2.1.1",
  "bin": {
    "i-read-u": "./bin/ireadu.js",
    "ireadu": "./bin/ireadu.js"
  },
  "main": "./bin/ireadu.js",
  "scripts": {
    "test": "jest --coverage",
    "prepare": "webpack --config webpack.common.js",
    "webpack-watch": "webpack --config webpack.dev.js --watch",
    "webpack-size": "webpack --config webpack.dev.js -p",
    "lint": "eslint src/**/* src/index.ts",
    "lint:fix": "eslint --fix src/**/* src/index.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/inquirer": "6.5.0",
    "@types/jest": "^24.0.17",
    "@types/marked": "^0.7.1",
    "@types/node": "^12.7.1",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "size-plugin": "^2.0.0",
    "terser": "^4.1.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^8.0.14",
    "typescript": "^3.5.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "commander": "^3.0.0",
    "inquirer": "^7.0.0",
    "marked": "^0.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s2terminal/i-read-u.git"
  },
  "bugs": {
    "url": "https://github.com/s2terminal/i-read-u/issues"
  },
  "homepage": "https://s2terminal.github.io/i-read-u/",
  "keywords": [
    "README",
    "markdown",
    "command",
    "parser"
  ],
  "license": "MIT"
}
